{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src/",
    "paths": {
      "/*": ["./*"]
    }
  }
}